    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "strict": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "cypress",
      "@types/jest",
      "@types/mocha",
      "node"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
